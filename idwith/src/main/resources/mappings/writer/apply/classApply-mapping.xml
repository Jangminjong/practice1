<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="WriterClassApplyDAO">
 	<resultMap type="writerClassApply" id="writerClassApplyResult">
 		<result property="classApplyId" column="class_apply_id"/>
 		<result property="classApplySeller" column="class_apply_seller"/>
 		<result property="classApplyName" column="class_apply_name"/>
 		<result property="classApplyPrice" column="class_apply_price"/>
 		<result property="classApplyPostAddress" column="class_apply_post_address"/>
 		<result property="classApplyAddress" column="class_apply_address"/>
 		<result property="classApplyDetailAddress" column="class_apply_detail_address"/>
 		<result property="classApplyReference" column="class_apply_reference"/>
 		<result property="classApplyDifficult" column="class_apply_difficult"/>
 		<result property="classApplyPerson" column="class_apply_person"/>
 		<result property="classApplyInfo" column="class_apply_info"/>
 		<result property="classApplyPhoto" column="class_apply_photo"  typeHandler="org.apache.ibatis.type.ArrayTypeHandler"/>
 		<result property="classApplyCheck" column="class_apply_check"/>
 	</resultMap>
 	<!-- 클래스신청 테이블에 등록  -->
 	<insert id="insertClassApply" parameterType="writerClassApply">
 		INSERT INTO class_apply (class_apply_id,
 								 class_apply_seller,
 								 class_apply_name,
 								 class_apply_price,
 								 class_apply_post_address,
 								 class_apply_address,
 								 class_apply_detail_address,
 								 class_apply_reference,
 								 class_apply_difficult,
 								 class_apply_person,
 								 class_apply_info,
 								 class_apply_photo,
 								 class_apply_check
 								 )
 		VALUES(#{classApplyId},
 			   #{classApplySeller},
 			   #{classApplyName},
 			   #{classApplyPrice},
 			   #{classApplyPostAddress},
 			   #{classApplyAddress},
 			   #{classApplyDetailAddress},
 			   #{classApplyReference},
 			   #{classApplyDifficult},
 			   #{classApplyPerson},
 			   #{classApplyInfo},
 			   #{classApplyPhoto, typeHandler=org.apache.ibatis.type.ArrayTypeHandler},
 			   0
 			   )
 	</insert>
 	<!-- 클래스 신청 목록 -->
 	<!-- <select id="getClassApplyList" resultType="writerClassApply" resultMap="writerClassApplyResult">
 		SELECT class_apply_name,
 			   class_apply_price,
 			   class_apply_post_address,
 			   class_apply_address,
 			   class_apply_detail_address,
 			   class_apply_reference,
 			   
 	</select> -->
 	
</mapper>